{
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet: build",
      "program": "${workspaceFolder}/src/ReleaseSync.Console/bin/Debug/net9.0/ReleaseSync.Console.dll",
      "args": [
        "sync",
        "-s",
        "2025-11-25T00:00:00+08:00",
        "-e",
        "2025-12-02T00:00:00+08:00",
        "--enable-gitlab",
        "--enable-bitbucket",
        "--enable-azure-devops",
        "--enable-export",
        "-o",
        ".\\output.json",
        "-f",
        "-v",
        "--enable-google-sheet",
        "--google-sheet-id",
        "1yl0VGxxxxxxxxxxwSdQY",
        "--google-sheet-name",
        "sheet1"
      ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    }
  ]

}
